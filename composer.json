{
    "name": "insolita/yii2-skeletest",
    "description": "Generate test skeletons for file or whole directory with target file methods (codeception/phpunit - based on template) ",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","codeception","phpunit","tests"],
    "license": "MIT",
    "version": "1.1.0",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "insolita",
            "email": "webmaster100500@ya.ru"
        }
    ],
    "require": {
        "php":">=7.0",
        "yiisoft/yii2": "~2.0.9",
        "insolita/yii2-validatorpack":"~0.0.1"
    },
    "require-dev":{
        "vlucas/phpdotenv": "*",
        "codeception/codeception": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3",
        "nesbot/carbon": "~1.9"
    },
    "autoload": {
        "psr-4": {
            "insolita\\skeletest\\": "src/"
        }
    }
}
